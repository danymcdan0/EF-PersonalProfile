@model IEnumerable<PersonalProfileUI.Models.DTOs.ExperienceDTO>

@{
}

<h1 class="mt-3">Education</h1>

<table class="table table-bordered">
	<thead>
		<tr>
			<th>Role</th>
			<th>Company</th>
			<th>Description</th>
			<th>Start Date</th>
			<th>End Date</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var experience in Model)
		{
			<tr>
				<td>@experience.Role</td>
				<td>@experience.Company</td>
				<td>@experience.Description</td>
				<td>@experience.StartDate</td>
				<td>@experience.EndDate</td>
			</tr>
		}
	</tbody>
</table>