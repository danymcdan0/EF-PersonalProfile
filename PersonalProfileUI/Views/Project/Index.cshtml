@model IEnumerable<PersonalProfileUI.Models.DTOs.ProjectDTO>

@{
}

<h1 class="mt-3">Education</h1>

<table class="table table-bordered">
	<thead>
		<tr>
			<th>Title</th>
			<th>Aim</th>
			<th>Description</th>
			<th>ImageUrl</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var project in Model)
		{
			<tr>
				<td>@project.Title</td>
				<td>@project.Aim</td>
				<td>@project.Description</td>
				<td>@project.ImageUrl</td>
			</tr>
		}
	</tbody>
</table>